<script type="text/javascript" charset="utf-8">
	document.observe("dom:loaded", function() {

		try{
			$('content').down('h2').insert({after:$('redmine-tour').cloneNode(true)});
			$('redmine-tour').remove();
		}catch(e){	/* do nothing. we're good where we are. */ }

		try{
			$$('#content > .contextual')[0].insert({before:$('redmine-tour-button').cloneNode(true)});
		}catch(e){
			var h2 = $$('#content > h2')[0];
			if(h2) {
			  $(h2).insert({before:$('redmine-tour-button').cloneNode(true)});
			}
		}finally{
			$('redmine-tour-button').remove();
		}
		
	});
</script>
